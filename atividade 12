#include <stdio.h>

int main() {
	int i;
    int A[10], B[10], M[10][2];

    // Leitura dos vetores A e B
    printf("Digite os elementos do vetor A:\n");
    for (i = 0; i < 10; i++) {
        scanf("%d", &A[i]);
    }

    printf("Digite os elementos do vetor B:\n");
    for (i = 0; i < 10; i++) {
        scanf("%d", &B[i]);
    }

    // Copiando os elementos para a matriz M
    for (i = 0; i < 10; i++) {
        M[i][0] = A[i];
        M[i][1] = B[i];
    }

    // Apresentando a matriz M
    printf("Matriz M:\n");
    for (i = 0; i < 10; i++) {
        printf("%d %d\n", M[i][0], M[i][1]);
    }

    return 0;
}
